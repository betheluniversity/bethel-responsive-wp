{% extends "tease.twig" %}

{% block content %}
    <div class="news__item" itemscope="itemscope" itemtype="http://schema.org/NewsArticle">
    {% if post.thumbnail.src %}
        <div class="news__image">
            <a href="{{post.link}}">
                <img src="{{post.thumbnail.src}}">
            </a>
        </div>
        <div class="news__content"><p class="news__headline">
                <a href="{{post.link}}">
                    <span itemprop="headline">{{post.title}}</span>
                </a>
            </p>
            <p>{{ post.post_date|date}} | {{ post.post_date|date('g:ia')}}</p>
            <p>{{post.get_preview(50)}}</p>
        </div>
    {% else %}
        <div class="news__image news__image--placeholder">
            <a href="{{post.link}}">
                <img src="data:image/png;base64,">
            </a>
        </div>
        <div class="news__content"><p class="news__headline">
                <a href="{{post.link}}">
                    <span itemprop="headline">{{post.title}}</span>
                </a>
            </p>
            <p>{{ post.post_date|date}} | {{ post.post_date|date('g:ia')}}</p>
            <p>{{post.get_preview(50)}}</p>
        </div>
    {% endif %}
    </div>
{% endblock %}